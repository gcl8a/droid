<?xml version="1.0"?>
<launch>
  <!-- differential_teleop_key already has its own built in velocity smoother -->
  <node pkg="droid" type="teleop_keyboard" name="teleop_keyboard"  output="screen">

    <param name="scale_linear" value="0.8" type="double"/>
    <param name="scale_angular" value="1.0" type="double"/>

  </node>
</launch>
